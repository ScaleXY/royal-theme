{
    "name": "scalexy/royal-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "ScaleXY\\RoyalTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScaleXY\\RoyalTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
